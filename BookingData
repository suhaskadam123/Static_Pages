	[Email] [nvarchar](50) NOT NULL,
	[ContactNumber] [nvarchar](50) NOT NULL,
	[TrekName] [nvarchar](50) NOT NULL,
	[NumberOfTrekkers] [int] NOT NULL,
	[TotalPrice] [int] NOT NULL,
